[sub_resource type="Shader" id="Shader_8pcxw"]
code = "shader_type spatial;
render_mode unshaded, fog_disabled;

//uniform vec2 target_resolution = vec2(512.0, 512.0);
uniform float target_resolution : hint_range(32.0, 512.0, 32.0) = 512.0;
uniform float colors_per_channel: hint_range(1.0, 128.0, 1.0) = 64.0;

uniform float outline_strength: hint_range(0.0, 1.0, 0.1) = 1.0; // Deprecated: Use new thickness_outline
uniform float normal_outline_strength: hint_range(0.0, 2.0, 0.1) = 0.5;
uniform vec4 outline_color : source_color = vec4(0.0, 0.0, 0.0, 1.0); // Black outline by default
uniform float outline_thickness : hint_range(0.1, 5.0, 0.1) = 1.0; // Thickness control

uniform sampler2D screen_texture: hint_screen_texture, filter_nearest;
uniform sampler2D depth_texture: hint_depth_texture, filter_nearest;
uniform sampler2D normal_texture: hint_normal_roughness_texture, filter_nearest;

void vertex() {
	POSITION = vec4(VERTEX.xy, 1.0, 1.0);
}

vec3 get_normal(vec2 uv) {
	return texture(normal_texture, uv).rgb;
}

float get_linear_depth(vec2 uv, mat4 inv_proj_mat) {
	float depth = texture(depth_texture, uv).r;
	vec3 ndc = vec3(uv * 2.0 - 1.0, depth);
	vec4 view = inv_proj_mat * vec4(ndc, 1.0);
	view.xyz /= view.w;
	return -view.z;
}

void fragment() {
	vec2 uv = floor(SCREEN_UV * target_resolution) / target_resolution;

	// Calculate offset based on thickness
	//vec2 offset = vec2(outline_thickness / target_resolution.x, outline_thickness / target_resolution.y);
	vec2 offset = vec2(outline_thickness / target_resolution, outline_thickness / target_resolution);


	vec2 uvs[5] = {
		uv,
		uv + vec2(0.0, 1.0) * offset, // Up
		uv - vec2(0.0, 1.0) * offset, // Down
		uv + vec2(1.0, 0.0) * offset, // Left
		uv - vec2(1.0, 0.0) * offset  // Right
	};


	float dc = get_linear_depth(uvs[0], INV_PROJECTION_MATRIX);
	float du = get_linear_depth(uvs[1], INV_PROJECTION_MATRIX);
	float dd = get_linear_depth(uvs[2], INV_PROJECTION_MATRIX);
	float dl = get_linear_depth(uvs[3], INV_PROJECTION_MATRIX);
	float dr = get_linear_depth(uvs[4], INV_PROJECTION_MATRIX);

	float h_depth = max(0.0, dl - dc) + max(0.0, dr - dc);
	float v_depth = max(0.0, du - dc) + max(0.0, dd - dc);

    // Adjust outline detection thresholds based on thickness
	float h_outline = step(0.1 * outline_thickness, h_depth) * step(0.7 * outline_thickness, distance(dl, dr));
	float v_outline = step(0.1 * outline_thickness, v_depth) * step(0.7 * outline_thickness, distance(du, dd));
	float depth_outline = 1.0 - step(0.01, h_outline + v_outline); // Removed outline_strength here

	vec3 nc = get_normal(uvs[0]);
	vec3 nu = get_normal(uvs[1]);
	vec3 nd = get_normal(uvs[2]);
	vec3 nl = get_normal(uvs[3]);
	vec3 nr = get_normal(uvs[4]);

	float nu_difference = distance(nc, nu) * max(0.0, 1.0 - distance(du, dc));
	float nd_difference = distance(nc, nd) * max(0.0, 1.0 - distance(dd, dc));
	float nl_difference = distance(nc, nl) * max(0.0, 1.0 - distance(dl, dc));
	float nr_difference = distance(nc, nr) * max(0.0, 1.0 - distance(dr, dc));

    //Adjust threshold based on thickness
	float h_inline = step(0.2 * outline_thickness, nl_difference + nr_difference);
	float v_inline = step(0.2 * outline_thickness, nu_difference + nd_difference);
	float normal_outline = step(0.1, h_inline + v_inline) * normal_outline_strength;


	vec3 color = texture(screen_texture, uv).rgb;
	vec3 quantized_color = floor(color * colors_per_channel) / colors_per_channel;

	// Blend the outline color with the base color
	float outline_factor = 1.0 - depth_outline * (1.0 + normal_outline);  // Calculate how much outline to show
    ALBEDO = mix(quantized_color, outline_color.rgb, outline_factor * outline_color.a);
}
"

